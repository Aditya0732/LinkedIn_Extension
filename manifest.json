{
    "manifest_version": 3,
    "name": "LinkedIn Profile Picture Modifier",
    "version": "1.0",
    "description": "Modifies profile pictures on the LinkedIn homepage and feed",
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["src/content_script.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["images/test.jpeg"],
        "matches": ["https://www.linkedin.com/*"]
      }
    ]
  }